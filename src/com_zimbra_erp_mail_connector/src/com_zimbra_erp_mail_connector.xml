<zimlet name="com_zimbra_erp_mail_connector" version="1.0" label="${msg.connector_panel_lbl}" description="VNC OpenERP Zimlet (VOZ)">
	<include>json2.js</include>
	<include>com_zimbra_erp_mail_connector.js</include>
	<include>configuration_setting.js</include>	
	<include>document_setting.js</include>	
	<include>push_to_openERP.js</include>
	<include>about_setting.js</include> 
	<include>VncContactSync.js</include>
	<include>contactsync.js</include>	
	<includeCSS>openerp.css</includeCSS>
	<resource>resources/submit_please_wait.gif</resource> 
	<resource>resources/about.png</resource>
	<resource>resources/configuration.png</resource>
	<resource>resources/document.png</resource>
	<resource>resources/VNC-Logo.png</resource>
	<resource>sync.png</resource>
	<resource>OpenERP.png</resource>
	
	<handlerObject>com_zimbra_erp_mail_connector_HandlerObject</handlerObject>
	
	<zimletPanelItem label="${msg.connector_panel_lbl}" icon="CalSync">
		
		<toolTipText>${msg.connector_panel_tooltip}</toolTipText>
		<dragSource type="ZmAppt" />
		<dragSource type="ZmContact" />
		<dragSource type="ZmConv" />
		<dragSource type="ZmMailMsg" />
		
	</zimletPanelItem>
	
	<userProperties>
		<property type="string" name="urladdress" value=""/>
		<property type="string" name="port" value=""/>
		<property type="string" name="getdatabase" value=""/>
		<property type="string" name="username" value=""/>
		<property type="string" name="userpassword" value=""/>
		<property type="string" name="cal_url" value=""/>
			
		<property type="string" name="openerp_id" value=""/>
		<property type="string" name="doc_list" value=""/>
		<property type="string" name="addBook" value=""/>
		<property type="string" name="addBookPath" value=""/>
		<property type="string" name="proto" value="http://"/>
			
	</userProperties>
	
	
	
</zimlet>
